<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rucafe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RUCafe">
        <activity
            android:name=".StoreOrderActivity"
            android:label="Store Order"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name=".OrderDetailActivity"
            android:label="Current Order"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name=".CoffeeOrderingActivity"
            android:label="Order Coffee"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name=".DonutOrderingActivity"
            android:label="Order Donut"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>

   